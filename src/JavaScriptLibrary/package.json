{
  "private": true,
  "name": "jabra.browser.integration-3.0",
  "scripts": {
    "build": "microbundle --name jabra --external none --no-compress && npm run dts-bundle && npm run copy-files && npm run rename",
    "start": "microbundle watch --name jabra --external none",
    "test": "jest",
    "dts-bundle": "dts-bundle --name jabra-browser-integration --main dist/index.d.ts --out ../jabra.browser.integration-3.0.d.ts",
    "copy-files": "copyfiles -u 1 dist/jabra.browser.integration-3.0.js dist/jabra.browser.integration-3.0.umd.js .",
    "rename": "npm run rename-es && npm run rename-umd",
    "rename-es": "renamer -f '/^jabra\\.browser\\.integration-3\\.0\\.js$/' -r 'jabra.browser.integration-3.0.es.js' * --force",
    "rename-umd": "renamer -f '/^jabra\\.browser\\.integration-3\\.0\\.umd\\.js$/' -r 'jabra.browser.integration-3.0.js' * --force"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "copyfiles": "^2.1.1",
    "dts-bundle": "^0.7.3",
    "jest": "^24.8.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.18.2",
    "renamer": "^1.1.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
